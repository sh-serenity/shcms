{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc-8 build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-lfcgi",
                "-ltidy",
                "-L",
                "/usr/lib/x86_64-linux-gnu",
                "-lmysqlclient",
                "-lpthread",
                "-ldl",
                "-lssl",
                "-lcrypto",
                "-lresolv",
                "-lm",
                "-lrt",
                "-lsodium",
                "-lcurl"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
       },
    ],
    "version": "2.0.0"
}